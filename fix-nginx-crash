sudo apt-get remove nginx-* && apt-get purge nginx-* && apt-get remove nginx nginx-common && apt-get purge nginx nginx-common && apt-get remove nginx-full nginx-common && apt-get purge php5-common && apt-get purge 'php5*' && apt-get purge 'nginx*' && rm -rf /etc/nginx && apt-get autoremove -y


